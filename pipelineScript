node {

      stage('Initialise') {
        /* Checkout the scripts */
        checkout scm: [
                $class: 'GitSCM',
                userRemoteConfigs: [
                        [
                                url: "https://github.com/gopalcs1988/PerformanceJmeter.git"
                        ]
                ],
                branches: [[name: "master"]]
        ], poll: false
      }

      stage('Complete any setup steps') {		
		dir("${WORKSPACE}\\VM_Update\\BatchScripts") {
			bat "upload_gws_to_vm.bat bsqav3rajagopal genesys genesys"		
      }      
    }
}
