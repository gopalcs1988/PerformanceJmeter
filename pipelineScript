node {

      stage('Initialise') {
        /* Checkout the scripts */
        checkout scm: [
                $class: 'GitSCM',
                userRemoteConfigs: [
                        [
                                url: "https://github.com/gopalcs1988/PerformanceJmeter.git"
                        ]
                ],
                branches: [[name: "master"]]
        ], poll: false
      }

      stage('Complete any setup steps') {
		echo "Complete set-up steps"
      }

      stage('Execute Performance Tests') {
        dir("${WORKSPACE}") {
			bat """
				call del C:/Users/rarumuga/Downloads/Softwares/apache-jmeter-5.4.1/bin/loginResult/*.* /s /q
				call del C:/Users/rarumuga/Downloads/Softwares/apache-jmeter-5.4.1/bin/loginResultHTML.zip /s /q
				call rmdir C:/Users/rarumuga/Downloads/Softwares/apache-jmeter-5.4.1/bin/loginResultHTML /q /s
				call mkdir C:/Users/rarumuga/Downloads/Softwares/apache-jmeter-5.4.1/bin/loginResultHTML
				call C:/Users/rarumuga/Downloads/Softwares/apache-jmeter-5.4.1/bin/jmeter.bat -n -t Login_Call.jmx -l C:/Users/rarumuga/Downloads/Softwares/apache-jmeter-5.4.1/bin/loginResult/result.csv -e -o C:/Users/rarumuga/Downloads/Softwares/apache-jmeter-5.4.1/bin/loginResultHTML
			"""	
        }
      }

      stage('Analyse Results') {
		echo "Analyse results"
      }
    }
